  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Program",
      "program": "${workspaceFolder}/src/index.ts",
      "preLaunchTask": "npm: build",
      "sourceMaps": true,
      "smartStep": true,
      "internalConsoleOptions": "openOnSessionStart",
      "cwd": "${workspaceFolder}",
      "protocol": "inspector",
      "outFiles": ["${workspaceFolder}/dist/**/*.js"],
      "outputCapture": "std",
      "env": {
        "NODE_ENV": "development",
        "PORT": "5000",
        "MONGODB_URI": "mongodb://localhost/bo_trading_new",
        "BINANCE_BASE_ENDPOINT": "wss://stream.binance.com:9443",
        "WS_COIN_API_ENDPOINT": "wss://ws.coinapi.io/v1/",
        "WS_COIN_API_KEY": "8EBF1655-8A50-4E22-9BA3-F96F80024D98"
      }
    }
  ]
